        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="hudlmo.interfaces.registerPage.Register">
            <intent-filter>
                <action android:name="hudlmo.interfaces.registerPage.Register" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="hudlmo.interfaces.mainmenu.Mainmenu"
            android:label="@string/title_activity_mainmenu"
            android:theme="@style/AppTheme" />
        <activity android:name="hudlmo.interfaces.VideoMain" />
        <activity android:name="hudlmo.interfaces.CreateMeeting.AddParticipants" />
        <activity android:name="hudlmo.interfaces.CreateMeeting.CreateMeeting" />
        <activity
